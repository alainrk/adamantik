{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:orig": "ts-node src/app.ts",
    "build": "tsc",
    "_dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-dev src/app.ts'",
    "_migrate": "npx prisma migrate dev --name XXXXXX",
    "dev": "tsx watch src/app.ts",
    "seed": "npx prisma migrate dev --name init",
    "studio": "npx prisma studio",
    "test": "vitest --pool=forks",
    "test:ci": "vitest --run --pool=forks",
    "nuke": "rm prisma/dev.db; rm -rf prisma/migrations"
  },
  "dependencies": {
    "@fastify/oauth2": "^7.8.0",
    "@prisma/client": "5.11.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "fastify": "4.26.2",
    "fastify-plugin": "^4.5.1",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "@types/node": "20.11.28",
    "borp": "^0.10.0",
    "prisma": "5.11.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.1",
    "typescript": "5.4.2",
    "vitest": "^1.4.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
